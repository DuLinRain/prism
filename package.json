{
  "name": "@kaleidawave/prism",
  "version": "1.1.0",
  "description": "Isomorphic web app compiler",
  "main": "node-out/index.js",
  "scripts": {
    "test": "jest",
    "build-web": "tsc -p tsconfig.web.json && rollup -c && npm run copy-bundle-web",
    "build-node": "tsc -p tsconfig.json && npm run copy-bundle",
    "copy-bundle-node": "xcopy /s /d /i \"src\\bundle\" \"node-out\\bundle\"",
    "copy-bundle-web": "xcopy /s /d /i \"src\\bundle\" \"web-out\\bundle\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaleidawave/prism.git"
  },
  "keywords": [
    "web-components",
    "ssr",
    "template",
    "isomorphic",
    "framework",
    "compiler"
  ],
  "author": "kaleidawave",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaleidawave/prism/issues"
  },
  "files": [
    "node-out"
  ],
  "homepage": "https://github.com/kaleidawave/prism#readme",
  "// Only used for testing and development:": "",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^25.2.3",
    "@types/jest-cli": "^24.3.0",
    "@types/node": "^13.13.25",
    "jest": "^25.5.4",
    "jest-cli": "^26.5.3",
    "rollup": "^2.32.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "ts-jest": "^26.4.1",
    "typescript": "^3.9.7"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "dependencies": {},
  "bin": {
    "prism": "out/index.js"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  }
}
